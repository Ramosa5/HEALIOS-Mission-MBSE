    }

    requirement def <R_SS_001_f> Sun_Pointing specializes R_M_001_e {
        doc /* The Space Segment shall maintain a sun-pointing attitude during the illuminated orbital phase to ensure consistent environmental conditions for the payload. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }

    requirement def <R_SS_001_g> Data_Storage specializes R_M_001_f {
        doc /* The Space Segment shall have sufficient onboard data storage capacity to store all collected data between ground contacts, with redundancy to prevent data loss. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }

    requirement def <R_SS_001_h> Eclipse_Detection specializes R_M_001_a {
        doc /* The Space Segment shall autonomously detect orbital eclipse periods to trigger self-healing cycles. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }
