    requirement def <R_SS_001> Self_Healing_Experiments specializes R_M_001 {
        doc /* The Space Segment shall enable active self-healing experiment performance on designated PSC samples by applying a controlled reverse-bias voltage during orbital eclipse phases. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;

        requirement : R_SS_001_a;
        requirement : R_SS_001_b; 
        requirement : R_SS_001_c;
        requirement : R_SS_001_d;
        requirement : R_SS_001_e;
        requirement : R_SS_001_f;
    }

    requirement def <R_SS_001_a> Payload_Accommodation specializes R_M_001_a {
        doc /* The Space Segment shall accommodate and support the operation of the perovskite solar cells payload. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }

    requirement def <R_SS_001_b> PV_Data_Gathering specializes R_M_001_c {
        doc /* The Space Segment shall autonomously gather, store, and timestamp electrical performance data (Voltage, Current) of all photovoltaic technologies onboard throughout the mission, with a frequency higher than once per orbit. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }

    requirement def <R_SS_001_c> Environment_Logging specializes R_M_001_d {

        doc /* The Space Segment shall log orbital environment parameters: thermal state, illumination condition, time since deployment, with a frequency higher than once per orbit. */
        subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
    }

    requirement def <R_SS_001_d> Voltage_Range specializes R_M_001_a {
            doc /* The Space Segment shall be capable to apply a reverse-bias voltage  to selected perovskite solar cells during eclipse periods in the range of 0 to -5 V with a resolution of at least 0.1 V. */
            subject Space_Segment references HealiosMissionInstancePackage::HEALIOS_Mission.HEALIOS_Space_Segment;
        }

    requirement def <R_SS_001_e> Reverse_Bias_Safety specializes R_M_001_a {
            doc /* The Space Segment shall ensure that reverse-bias conditions applied during self-healing cycles over the mission lifetime do not exceed material degradation thresholds of solar cells. */
